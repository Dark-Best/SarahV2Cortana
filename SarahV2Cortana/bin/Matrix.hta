 <SCRIPT Language="VBScript">
 Sub Window_onLoad
 HereThere.style.visibility="hidden"
 Parler("Mise en veille de la Matrix !")
 End Sub
     
 Sub Focus
 Password.value="0000"
 Password.Focus
 End Sub
     
 Sub ToggleButton
 If HereThere.style.visibility="hidden" Then
 HereThere.style.visibility="visible"
 Else
 HereThere.style.visibility="hidden"
 End If
 End Sub
     
 Sub Pointer
 document.body.style.cursor = "hand"
 End Sub
     
 Sub DefaultCursor
 document.body.style.cursor = "default"
 End Sub
 
 Sub VERIFY
 IF Password.value="" THEN
 Message = " Le mot de passe est vide !"
 Parler(Message)
 MsgBox Message,48,Message
 ' Location.Reload(True)
 HereThere.style.visibility="visible"
 Focus
 End if
 If Password.value <> "123" Then
 Message = " Mot de passe incorrect !"
 Parler(Message)
 MsgBox Message,16,Message
 'Location.Reload(True)
 HereThere.style.visibility="visible"
 Focus
 End if
 If Password.value = "123" Then
 window.close
 End if
 End Sub
     
 Sub Hand
 document.body.style.cursor = "hand"
 End Sub
     
 Sub DefaultCursor
 document.body.style.cursor = "default"
 End Sub
     
 Sub Parler(Message)
 Set Voix = CreateObject("SAPI.SpVoice")
 Voix.Speak Message
 End Sub
 </SCRIPT>
     
 <SCRIPT LANGUAGE="JavaScript">
 function document.onkeydown() {  
 var alt=window.event.altKey;
 if (event.keyCode==116 || event.keyCode==27 || alt && event.keyCode==115) {
 event.keyCode=0;
 event.cancelBubble=true;
 return false; }
            }
 </SCRIPT>

